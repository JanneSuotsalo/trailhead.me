{{> navigation}}

<content>
  <div id="wrapper">
    <div class="feed">
      {{#each posts}}
      <div class="post">
        {{#each media}}
        <a href="/file/{{this}}">
          <div id="image" style="background-image: url(/file/{{this}}/m);"></div>
        </a>
        {{/each}}
        <p>{{text}}</p>
        <span>{{createdAt}}</span>
        <a href="/{{username}}/{{postID}}">View</a>
      </div>
      {{/each}}
    </div>
  </div>



</content>